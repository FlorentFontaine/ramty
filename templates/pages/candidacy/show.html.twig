{% extends 'base.html.twig' %}

{% block title %}Candidacy{% endblock %}

{% block body %}
    <h1>Candidacy</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ candidacy.id }}</td>
            </tr>
            <tr>
                <th>Title</th>
                <td>{{ candidacy.title }}</td>
            </tr>
            <tr>
                <th>Content</th>
                <td>{{ candidacy.content }}</td>
            </tr>
            <tr>
                <th>Created_at</th>
                <td>{{ candidacy.createdAt ? candidacy.createdAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>Updated_at</th>
                <td>{{ candidacy.updatedAt ? candidacy.updatedAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_candidacy_index') }}">back to list</a>

    <a href="{{ path('app_candidacy_edit', {'id': candidacy.id}) }}">edit</a>

    {{ include('candidacy/_delete_form.html.twig') }}
{% endblock %}
